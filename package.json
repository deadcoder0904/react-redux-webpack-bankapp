{
  "name": "react-redux-webpack-bankapp",
  "version": "1.0.0",
  "description": "Simple React Redux Bank App using Webpack",
  "main": "index.js",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --recursive --require ./test/test_helper.js",
    "test:watch": "npm run test -- --watch",
    "production": "webpack -p",
    "uglify": "npm run production && uglifyjs ./public/bundle.min.js -c -m -o ./public/bundle.min.js --source-map ./public/bundle.min.js.map",
    "start": "webpack-dev-server",
    "dev": "webpack-dev-server --progress --inline --hot"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bootstrap": "^3.3.6",
    "immutable": "^3.8.1",
    "react": "^15.4.0",
    "react-bootstrap": "^0.30.6",
    "react-dom": "^15.4.0",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2"
  },
  "devDependencies": {
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "chai": "^3.5.0",
    "chai-immutable": "^1.6.0",
    "css-loader": "^0.26.0",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.19.0",
    "mocha": "^3.0.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "style-loader": "^0.13.1",
    "uglify-js": "^2.7.5",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
